{
  "name": "sb-http-client",
  "version": "0.1.0",
  "description": "MyServiceBus http client",
  "main": "lib/index.js",
  "module": "lib/index.es.js",
  "typings": "lib/src/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "clean": "rimraf lib/",
    "test": "cross-env TS_NODE_PROJECT='./tests/tsconfig.json' mocha --require ts-node/register tests/**/*.ts",
    "start": "node ./lib/index.js",
    "compile": "tsc -p .",
    "copy": "copyfiles -u 1 src/**/*.proto lib/",
    "build": "npm run clean && npm run copy && rollup -c rollup.config.js",
    "prepare": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.1.0",
    "chai": "^4.3.6",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "mocha": "^9.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.67.0",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-node": "^10.5.0"
  },
  "dependencies": {
    "@types/node": "^17.0.16",
    "@youtoken/logger": "^1.5.24",
    "@youtoken/types": "^1.0.84",
    "axios": "^0.25.0",
    "node-fetch": "^3.2.0",
    "protobufjs": "^6.11.2",
    "typescript": "^4.5.5"
  }
}
